// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Paciente{
    id Int @id @default(autoincrement())
    name String
    birth_date String
    phone String
    cpf String
    profession String
    email String
    address String
    how_know_us String
}

